{
  "name": "xiyou-wrapper",
  "version": "0.1.0",
  "private": true,
  "description": "Desktop app wrapper for https://student.xiyouyingyu.com/ with JS/CSS injection",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "pack": "electron-packager . xiyou-wrapper --platform=win32 --arch=x64 --out=dist --overwrite",
    "pack:win": "electron-packager . xiyou-wrapper --platform=win32 --arch=x64 --out=dist --overwrite",
  "pack:min": "electron-packager . xiyou-wrapper --platform=win32 --arch=x64 --out=dist --overwrite --asar --prune=true --no-deref-symlinks"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "electron-store": "^9.0.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.5",
    "electron": "^31.0.0",
    "electron-packager": "^17.1.2"
  }
}
